libpcre.a:	objs/Makefile
	test -d '$(DESTDIR)$(PREFIX)/../bin/$(PCRE)/lib' \
	|| echo required $(PCRE)


ssl.h:	objs/Makefile
	test -d '$(DESTDIR)$(PREFIX)/../bin/$(OPENSSL)/lib' \
	|| echo required $(OPENSSL)


libz.a:	objs/Makefile
	test -d '$(DESTDIR)$(PREFIX)/../bin/$(ZLIB)/lib' \
	|| echo required $(ZLIB)


manpage:	objs/nginx.8

objs/nginx.8:	man/nginx.8
	sed -e "s|%%PREFIX%%|$(PREFIX)|" \
		-e "s|%%PID_PATH%%|$(PREFIX)/run|" \
		-e "s|%%CONF_PATH%%|$(PREFIX)/conf|" \
		-e "s|%%ERROR_LOG_PATH%%|$(PREFIX)/logs|" \
		< man/nginx.8 > $@

install:	build 
	mkdir -p '$(DESTDIR)$(PREFIX)'
	mkdir -p '$(DESTDIR)$(PREFIX)/sbin'
	cp objs/nginx '$(DESTDIR)$(PREFIX)/sbin'
	mkdir -p '$(DESTDIR)$(PREFIX)/conf'
	cp conf/koi-win '$(DESTDIR)$(PREFIX)/conf'
	cp conf/koi-utf '$(DESTDIR)$(PREFIX)/conf'
	cp conf/win-utf '$(DESTDIR)$(PREFIX)/conf'
	cp conf/mime.types '$(DESTDIR)$(PREFIX)/conf'
	cp conf/mime.types '$(DESTDIR)$(PREFIX)/conf/mime.types.default'
	cp conf/fastcgi_params '$(DESTDIR)$(PREFIX)/conf'
	cp conf/fastcgi_params '$(DESTDIR)$(PREFIX)/conf/fastcgi_params.default'
	cp conf/fastcgi.conf '$(DESTDIR)$(PREFIX)/conf'
	cp conf/fastcgi.conf '$(DESTDIR)$(PREFIX)/conf/fastcgi.conf.default'
	cp conf/uwsgi_params '$(DESTDIR)$(PREFIX)/conf'
	cp conf/uwsgi_params '$(DESTDIR)$(PREFIX)/conf/uwsgi_params.default'
	cp conf/scgi_params '$(DESTDIR)$(PREFIX)/conf'
	cp conf/scgi_params '$(DESTDIR)$(PREFIX)/conf/scgi_params.default'
	cp conf/nginx.conf '$(DESTDIR)$(PREFIX)/conf'
	cp conf/nginx.conf '$(DESTDIR)$(PREFIX)/conf/nginx.conf.default'
	mkdir -p '$(DESTDIR)$(PREFIX)/scgi_temp'
	mkdir -p '$(DESTDIR)$(PREFIX)/uwsgi_temp'
	cp -R html '$(DESTDIR)$(PREFIX)'
	mkdir -p '$(DESTDIR)$(PREFIX)/fastcgi_temp'
	mkdir -p '$(DESTDIR)$(PREFIX)/proxy_temp'
	mkdir -p '$(DESTDIR)$(PREFIX)/client_body_temp'
	mkdir -p '$(DESTDIR)$(PREFIX)/logs'
	mkdir -p '$(DESTDIR)$(PREFIX)/run'
